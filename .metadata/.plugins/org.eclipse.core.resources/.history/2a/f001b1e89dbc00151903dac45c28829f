#ifndef SHOOTER_HPP
#define SHOOTER_HPP

#include "Schematic.hpp"
#include "ShooterVision.hpp"

class Shooter {
private:
	ShooterVision *vision;
	bool activated;

public:
	Shooter() {
		vision = new ShooterVision();
		activated = true;
	}

	void Update();
	void Dashboard();
	bool Activated();
};

#endif
