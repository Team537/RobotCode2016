#ifndef SRC_CROSSREACHAUTO_H_
#define SRC_CROSSREACHAUTO_H_

#include "Schematic.hpp"
#include <DriveTrain/DriveTrain.hpp>
#include <Shooter/ShooterManager.hpp>

class CrossReachAuto {
private:
	ShooterManager *shoot;
	DriveTrain *driveTrain;
public:
	CrossReachAuto(SendableChooser *chooser, bool defaultChooser, DriveTrain *driveTrain) : IAutonomous(chooser, defaultChooser, "Test")
	{
		reachState = 0;
		this->driveTrain = driveTrain;
	}

	void Start();
	bool Run(double time);
	void Stop();

};


