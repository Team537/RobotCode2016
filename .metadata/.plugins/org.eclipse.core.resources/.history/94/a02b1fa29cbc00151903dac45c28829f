#ifndef VISIONNETWORK_HPP
#define VISIONNETWORK_HPP

#include "Schematic.hpp"

class VisionNetwork {
private:
	std::shared_ptr<NetworkTable> goalImageTable;
	std::vector<double> readings;

public:
	VisionNetwork(std::string table) {
		goalImageTable = NetworkTable::GetTable(table);
	}

	void Update();
	void Dashboard();
	double GetXOffset();
};

#endif
