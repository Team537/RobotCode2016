#include <AutoReach.hpp>

AutoReach::AutoReach() {
	// TODO Auto-generated constructor stub

}

AutoReach::~AutoReach() {
	// TODO Auto-generated destructor stubs
}

void AutoReach::Init()
{
	clock->Reset();
	clock->Start();
}

void AutoReach::Run()
{
	if (clock->Get() >= 3)
	{
		clock->Stop();
		drive->AutoDrive(0);
	}
	else
	{
		drive->AutoDrive(1);
	}
}

