#ifndef DRIVEOUTPUT_HPP
#define DRIVEOUTPUT_HPP

#include <Schematic.hpp>
#include <PIDOutput.h>

class DriveAngle: public PIDOutput
{
    private:
        float output;
    public:
        DriveAngle() :
                PIDOutput()
        {
            output = 0;
        }

        void PIDWrite(float output)
        {
            this->output = output;
        }

        float Get()
        {
            return output;
        }
};

#endif
