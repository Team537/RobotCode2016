#ifndef DRIVEOUTPUT_HPP
#define DRIVEOUTPUT_HPP

#include <Schematic.hpp>
#include <PIDOutput.h>

class DriveOutput: public PIDOutput
{
    private:
        float output;
    public:
        DriveOutput() :
                PIDOutput()
        {
            output = 0;
        }

        void PIDWrite(float output)
        {
this->output = output;
        }

        float Get()
        {
            return output;
        }
};

#endif
