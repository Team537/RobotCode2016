#include <Toolbox/TakeBackHalf.hpp>

void TakeBackHalf::Enable()
{
    timer->Reset();
    timer->Start();
}

void TakeBackHalf::Disable()
{
    timer->Stop();
}

void TakeBackHalf::Update()
{
}

float TakeBackHalf::GetInput()
{
    return input;
}

void TakeBackHalf::SetInput(float input)
{
    this->input = input;
}

float TakeBackHalf::GetOutput()
{
    Update();
    return output;
}
