#ifndef SRC_COLLECTOR_HPP_
#define SRC_COLLECTOR_HPP_

#include <WPILib.h>

class Collector
{
private:
	Solenoid *Pistons;
	Victor *Motor;
	int btnState = 0;
	int speed = 0;
	Collector()
	{
		Pistons = new Solenoid(1);
		Motor = new Victor(1);
	}
public:
	void Extend(bool BTN);
	void Collect(bool BTN1, bool BTN2);
};

#endif
