#include <AutoReach.h>

AutoReach::AutoReach() {
	// TODO Auto-generated constructor stub

}

AutoReach::~AutoReach() {
	// TODO Auto-generated destructor stubs
}

void AutoReach::Init()
{
	clock->Reset();
	clock->Start();
}

void AutoReach::Run(float speed)
{

	if (clock->Get() >= 3)
	{
		clock->Stop();
		speed = 0;
	}
	else()
	{
		speed = 1;
	}
	return speed;
}

