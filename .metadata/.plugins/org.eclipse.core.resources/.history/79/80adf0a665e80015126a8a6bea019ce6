#include <Autonomous/AutonomousTimed.hpp>

void AutonomousTimed::Start()
{
    AutoTime->Start();
    driveTrain->AutoTimed();
}

bool AutonomousTimed::Run(double time)
{
    if(AutoTime->Get() > 2)
    {
        driveTrain->SetState(DriveTrain::DriveState::NONE);
    }
}

void AutonomousTimed::Stop()
{
    driveTrain->SetState(DriveTrain::DriveState::NONE);
}
