#include <Shooter/ShooterManager.hpp>

void ShooterManager::Update()
{
	vision->Update();

	if (activateButton->WasDown())
	{
		activated = !activated;
	}
}

void ShooterManager::Dashboard()
{
	vision->Dashboard();
	SmartDashboard::PutBoolean("Shooter Activated", activated);
}

void ShooterManager::CleanUp()
{

}
